<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Séquence pour les clients -->
        <record id="sequence_it_client" model="ir.sequence">
            <field name="name">Séquence Client IT</field>
            <field name="code">it.client</field>
            <field name="prefix">CLI/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Séquence pour les équipements -->
        <record id="sequence_it_equipment" model="ir.sequence">
            <field name="name">Séquence Équipement IT</field>
            <field name="code">it.equipment</field>
            <field name="prefix">EQ/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Séquence pour les logiciels -->
        <record id="sequence_it_software" model="ir.sequence">
            <field name="name">Séquence Logiciel IT</field>
            <field name="code">it.software</field>
            <field name="prefix">SW/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Séquence pour les contrats -->
        <record id="sequence_it_contract" model="ir.sequence">
            <field name="name">Séquence Contrat IT</field>
            <field name="code">it.contract</field>
            <field name="prefix">CTR/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Séquence pour les interventions -->
        <record id="sequence_it_intervention" model="ir.sequence">
            <field name="name">Séquence Intervention IT</field>
            <field name="code">it.intervention</field>
            <field name="prefix">INT/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Séquence pour les alertes -->
        <record id="sequence_it_alert" model="ir.sequence">
            <field name="name">Séquence Alerte IT</field>
            <field name="code">it.alert</field>
            <field name="prefix">ALT/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>